{
  "name": "mastani",
  "version": "0.0.1",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "dist": "next build && next export",
    "ts": "concurrently \"tsc --watch\" \"next ./bin\""
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "transform-decorators-legacy",
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "tabWidth": 2,
    "semi": false
  },
  "dependencies": {
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "next": "3.0.6",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "styled-components": "^2.1.2"
  },
  "license": "TODO",
  "devDependencies": {
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.5",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "concurrently": "^3.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "typescript": "^2.4.2"
  }
}

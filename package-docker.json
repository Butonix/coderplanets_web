{
  "name": "coderplanets_web",
  "version": "0.4.0",
  "license": "Apache License 2.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev run-p server lint:watch",
    "prod": "cross-env NODE_ENV=production run-p server lint:watch",
    "local": "cross-env NODE_ENV=local run-p server",
    "server": "npm run clean && node server.js",
    "build": "run-s dist clean",
    "build:only": "npm run i18n && next build",
    "launch": "cross-env NODE_ENV=production next build && cross-env NODE_ENV=production next start -p 3000",
    "launch-server": "cross-env NODE_ENV=production next build && cross-env NODE_ENV=production node server",
    "build:dev": "cross-env NODE_ENV=dev run-s dist:dev clean",
    "dist": "npm run i18n && next build && next export -o \"build\"",
    "i18n": "shjs ./utils/scripts/create_default_lang.js && shjs ./utils/scripts/cp_locales.js",
    "start": "NODE_ENV=production node server.js",
    "clean": "shjs ./utils/scripts/clean.js",
    "precommit": "lint-staged",
    "gen": "plop --plopfile ./utils/scripts/generators/index.js",
    "analyze": "cross-env ANALYZE=1 next build",
    "lint": "eslint --fix pages/**/*.js components/**/* containers/**/* stores/**/* utils/**/*",
    "lint:watch": "esw -w pages/**/*.js components/**/* containers/**/* stores/**/* utils/**/*",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "test:cover": "npm run test -- --coverage",
    "test:e2e": "cypress open",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "npm run lint"
    ]
  },
  "dependencies": {
    "accepts": "^1.3.4",
    "antd": "3.1.6",
    "apollo-cache-inmemory": "1.1.11",
    "apollo-client": "2.2.7",
    "apollo-link": "1.2.1",
    "apollo-link-error": "^1.0.9",
    "apollo-link-http": "1.5.3",
    "apollo-link-retry": "2.2.3",
    "chalk": "^2.4.1",
    "debug": "3.1.0",
    "draft-js": "^0.10.5",
    "draft-js-mention-plugin": "^3.0.4",
    "draft-js-plugins-editor": "^2.0.4",
    "glob": "^7.1.2",
    "graphql": "^0.13.2",
    "graphql-request": "^1.6.0",
    "graphql-tag": "^2.6.1",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "mastani-codehighlight": "0.0.5",
    "mobx": "4.3.1",
    "mobx-react": "5.2.3",
    "mobx-state-tree": "2.2.0",
    "module-alias": "^2.0.1",
    "next": "6.1.0",
    "path-match": "^1.2.4",
    "polished": "1.9.2",
    "prop-types": "^15.5.10",
    "pubsub-js": "^1.6.0",
    "ramda": "0.25.0",
    "randomcolor": "^0.5.3",
    "react": "16.4.1",
    "react-beautiful-dnd": "6.0.0",
    "react-calendar-heatmap": "^1.6.3",
    "react-click-outside": "^3.0.1",
    "react-content-loader": "3.1.1",
    "react-dom": "16.4.1",
    "react-icons": "2.2.5",
    "react-intl": "2.4.0",
    "react-json-view": "1.17.1",
    "react-keydown": "^1.9.4",
    "react-masonry-component": "^6.0.1",
    "react-svg": "4.0.17",
    "react-tooltip": "^3.4.0",
    "react-trend": "^1.2.4",
    "react-waypoint": "^8.0.0",
    "remarkable": "^1.7.1",
    "remarkable-emoji": "^0.1.3",
    "remarkable-mentions": "^0.1.0",
    "rxjs": "6.2.1",
    "rxjs-compat": "6.2.1",
    "scroll-into-view-if-needed": "1.5.0",
    "shortid": "^2.2.8",
    "store": "^2.0.12",
    "styled-components": "3.3.3",
    "timeago-react": "2.0.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-import": "1.8.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-ramda": "1.6.1",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "cross-env": "^5.0.5",
    "dotenv": "6.0.0",
    "dotenv-webpack": "^1.5.4",
    "webpack-bundle-analyzer": "2.13.1"
  }
}
